generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuario {
  id         Int     @id @default(autoincrement())
  correo     String? @db.VarChar(30)
  contrase_a String? @map("contrase√±a") @db.VarChar(50)
}

model apartamento {
  id              Int     @id @default(autoincrement())
  torre           String? @db.VarChar(30)
  bloque          String? @db.VarChar(30)
  piso            String? @db.VarChar(30)
  tipoapartamento String? @db.VarChar(30)
}

